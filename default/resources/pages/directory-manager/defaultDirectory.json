{
  "$id": "defaultDirectory",
  "$schema": "https://schema.yext.com/config/pages/directory-manager/v1",
  "name": "Directory",
  "baseEntities": {
    "entityTypes": [
      "location"
    ]
  },
  "root": {
    "entityType": "ce_root",
    "slug": "index.html",
    "entityTemplateId": "directoryRoot",
    "fieldsToCopyFromBase": [
      "c_brand"
    ]
  },
  "localization": {
    "locales": ["en"]
  },
  "levels": [
    {
      "entityType": "ce_region",
      "field": "address.region",
      "slug": "/{{address.region}}",
      "entityTemplateId": "directoryRegion",
      "fieldsToCopyFromBase": [
        "c_brand"
      ]
    },
    {
      "entityType": "ce_city",
      "field": "address.city",
      "slug": "/{{address.region}}/{{address.city}}",
      "entityTemplateId": "directoryCity",
      "fieldsToCopyFromBase": [
        "c_brand"
      ]
    }
  ]
}