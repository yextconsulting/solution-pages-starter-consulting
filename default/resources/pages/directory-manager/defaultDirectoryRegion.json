{
  "$id": "defaultDirectoryRegion",
  "$schema": "https://schema.yext.com/config/pages/directory-manager/v1",
  "name": "Directory with region",
  "baseEntities": {
    "entityTypes": [
      "location"
    ],
    "savedFilterIds": [
      "1341084605"
    ]
  },
  "root": {
    "entityType": "ce_root",
    "slug": "index.html",
    "entityTemplateId": "directoryRoot"
  },
  "localization": {
    "locales": ["en"]
  },
  "levels": [
    {
      "entityType": "ce_country",
      "field": "address.countryCode",
      "slug": "/{{address.countryCode}}",
      "entityTemplateId": "directoryCountry"
    },
    {
      "entityType": "ce_region",
      "field": "address.region",
      "slug": "/{{address.countryCode}}/{{address.region}}",
      "entityTemplateId": "directoryRegion"
    },
    {
      "entityType": "ce_city",
      "field": "address.city",
      "slug": "/{{address.countryCode}}/{{address.region}}/{{address.city}}",
      "entityTemplateId": "directoryCity"
    }
  ]
}
