{
  "$id": "site",
  "$schema": "https://schema.yext.com/config/pages/yextsite-config/v1",
  "siteName": "Site",
  "repoConfig": "starter-repo",
  "siteSettings": {
    "autoDeploy": true,
    "autoStage": true,
    "autoPublish": false,
    "pullRequestsEnabled": false,
    "stagingCredentials": {
      "password": "",
      "username": ""
    }
  },
  "branches": [
    {
      "name": "main",
      "isProductionBranch": true,
      "envVars": [
        {
          "key": "YEXT_PUBLIC_SEARCH_API_KEY",
          "value": "${{searchAPIKey}}"
        },
        {
          "key": "YEXT_PUBLIC_NEARBY_API_KEY",
          "value": "${{nearbyAPIKey}}"
        }
      ]
    }
  ]
}